{
  "expo": {
    "name": "My Flashcards",
    "description": "This app is used to create new flashcards.",
    "slug": "mobile-flashcards",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.narendra.myflashcards"
    },
    "android": {
      "package": "io.narendra.myflashcards"
    }
  }
}
